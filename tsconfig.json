{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "typeRoots": ["node_modules/@types", "types", "./types"],
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
